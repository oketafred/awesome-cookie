  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "jsdom": "^24.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.5",
    "vitest": "^1.3.1"
